<body>
    <div class="row align-items-center main-bg">
        <div class="col-md-4 text-center">
          <form class="login-form" action = "/dept-admin-dashboard/<%= institute %>/<%= dept %>/addcourse" method = "POST">
          <div style="font-size: 40px; font-weight: bold; color:#5c6bc0">SignUp</div>
          <br>
            <div class="form-group form-label">
              <label class="form-label-2" for="exampleInputUsername">Course Name</label>
              <input type="text" name="course" required class="form-control" placeholder="Enter Course Name">
            </div>
            <button type="submit" class="btn btn1">AddCourse</button>
            <br>
            <br>
            <a class="text-links" href="login.html"><span style="font-size: 16px;">Already Have Account? <u>Login Here</u></span></a>
          </form>
        </div>
      </div>
    </div>
  
      <script type="text/javascript">
      
      var pass = document.getElementById('pass');
      function myfunc(checkbox){
        if(checkbox.checked == true){
          pass.setAttribute("type","text"); 
        } else {
        pass.setAttribute("type","password");
        }
      }
      
      </script>  
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
